<div class="books">

  <h1 class="u-margin-Bm">Login</h1>

  <%= form_for @admin_session, :url => admin_session_path do |f| %>
    <p>
      <%= f.label :login %><br />
      <%= f.text_field :login %>
    </p>
   
    <p>
      <%= f.label :password %><br />
      <%= f.password_field :password %>
    </p>
   
    <%= f.submit "Login" %>
  <% end %>

</div>